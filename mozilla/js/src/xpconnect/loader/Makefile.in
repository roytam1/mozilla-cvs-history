DEPTH 		= ../../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= jsloader
LIBRARY_NAME	= jsloader_s
FORCE_STATIC_LIB = 1
REQUIRES	= xpcom \
		  string \
		  xpconnect \
		  js \
		  caps \
		  dom \
		  necko \
		  $(NULL)

CPPSRCS		= mozJSComponentLoader.cpp mozJSSubScriptLoader.cpp

include $(topsrcdir)/config/rules.mk

DEFINES		+= -DJSFILE -DJS_THREADSAFE


