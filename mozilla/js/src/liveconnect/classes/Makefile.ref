DEPTH = ../..

include $(DEPTH)/config.mk
JAR = $(JDK)/bin/jar
include $(DEPTH)/liveconnect/config/$(OS_CONFIG).mk

JARS     = $(LCJAR)

JARS    := $(addprefix $(CLASSDIR)/$(OBJDIR)/, $(JARS))

PREDIRS  = netscape
TARGETS += $(JARS)

include $(DEPTH)/rules.mk

$(CLASSDIR)/$(OBJDIR)/$(LCJAR):
	cd $(CLASSDIR)/$(OBJDIR) ; \
	  $(JAR) cvf $(@F) netscape
