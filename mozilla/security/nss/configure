    _TEST=`grep NCR /etc/bcheckrc | head -1 2>/dev/null`
    if echo "$_TEST" | grep -c NCR 2>/dev/null; then
        OS_ARCH=NCR
    else
        OS_ARCH=UNIXWARE
    fi
    OS_RELEASE=`uname -v`
fi

