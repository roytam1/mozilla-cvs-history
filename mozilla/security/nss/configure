    OS_ARCH=NEC
fi

if test "${OS_ARCH}" = "AIX"; then
    OS_RELEASE=`uname -v`.`uname -r`
fi

# Distinguish between OSF1 V4.0B and V4.0D
if test "${OS_ARCH}${OS_RELEASE}" = "OSF1V4.0"; then
    OS_VERSION=`uname -r`
    if test "${OS_VERSION}" = "564"; then
        OS_RELEASE="V4.0B"
    fi
    if test "${OS_VERSION}" = "878"; then
        OS_RELEASE="V4.0D"
    fi
fi
 
