	.\$(OBJDIR)\DocumentFunctionCall.obj		\
	.\$(OBJDIR)\ElementAvailableFnCall.obj		\
	.\$(OBJDIR)\FunctionAvailableFnCall.obj		\
	.\$(OBJDIR)\GenerateIDFunctionCall.obj		\
