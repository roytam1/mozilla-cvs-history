	CurrentFunctionCall.cpp			\
	DocumentFunctionCall.cpp		\
	ElementAvailableFnCall.cpp		\
	FunctionAvailableFnCall.cpp		\
	GenerateIDFunctionCall.cpp		\
	SystemPropertyFunctionCall.cpp	\
	$(NULL)

CPP_OBJS=						\
