    <radiogroup id="P3PLevel"
                preftype="int" prefstring="P3P.level" prefattribute="data">
      <radio value="0"
             label="&lowlevel.label;"
             checked="true"
             oncommand="DoMapLevel();"/>
      <radio value="1"
             group="P3PLevel"
             label="&mediumlevel.label;"
             oncommand="DoMapLevel();"/>
      <radio value="2"
             group="P3PLevel"
             label="&highlevel.label;"
             oncommand="DoMapLevel();"/>
      <radio value="3" id="custradio"
             group="P3PLevel"
             label="&customlevel.label;"
             oncommand="DoMapLevel();"/>
      <separator/>
      <hbox>
        <button id="customizebutton" value="&customize.label;"
                accesskey="&custom.accesskey;"
                oncommand="doCustomize();"/>
        <spacer flex="1"/>
      </hbox>
    </radiogroup>
  </groupbox>

