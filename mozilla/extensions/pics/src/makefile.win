DEPTH=..\..\..
include <$(DEPTH)/config/config.mak>


DEFINES=-D_IMPL_NS_PICS -DWIN32_LEAN_AND_MEAN
MODULE=pics 
LIBRARY_NAME=pics
COMPONENT_NAME=nsPICSModule

CSRCS=				\
 cslabel.c	\
 csparse.c	\
 htchunk.c	\
 htlist.c	\
 htstring.c	\
 lablpars.c	\
 $(NULL)

CPPSRCS=				\
 nsPICS.cpp			\
 nsPICSElementObserver.cpp			\
 $(NULL)

C_OBJS=				\
 .\$(OBJDIR)\cslabel.obj	\
 .\$(OBJDIR)\csparse.obj	\
 .\$(OBJDIR)\htchunk.obj	\
 .\$(OBJDIR)\htlist.obj	\
 .\$(OBJDIR)\htstring.obj	\
 .\$(OBJDIR)\lablpars.obj	\
 $(NULL)


CPP_OBJS=				\
 .\$(OBJDIR)\nspics.obj	\
 .\$(OBJDIR)\nsPICSElementObserver.obj	\
 $(NULL)

LINCS = -I$(PUBLIC)/js \
		-I$(PUBLIC)\pics \
		-I$(PUBLIC)\xpcom \
		-I$(PUBLIC)\raptor \
		-I$(PUBLIC)\pref \
		-I$(DEPTH)\include \

LCFLAGS = \
        $(LCFLAGS) \
        $(DEFINES) \
        $(NULL)

# These are the libraries we need to link with to create the dll
LLIBS	= \
 $(LIBNSPR) \
 $(DIST)\lib\js$(MOZ_BITS)$(VERSION_NUMBER).lib \
 $(DIST)\lib\xpcom.lib \
 $(DIST)\lib\xpcom.lib \
 $(DIST)\lib\gkparser.lib \
 $(HASHLIBS)

include <$(DEPTH)\config\rules.mak>

