LIBRARY_NAME = widgetsupport_s
#DEFINES =-D_IMPL_NS_WIDGET

CPPSRCS = \
	nsWidgetSupport.cpp      \
	$(NULL)

EXPORTS = \
	nsWidgetSupport.h \
	$(NULL)

MODULE=widget 
REQUIRES	= xpcom \
		  string \
		  gfx \
		  $(NULL)

OBJS= \
	.\$(OBJDIR)\nsWidgetSupport.obj  \
	$(NULL)


LCFLAGS =                               \
	$(LCFLAGS)                      \
  $(DEFINES) \
	$(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(LIBRARY)
	$(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib

clobber::
	rm -f $(DIST)\lib\$(LIBRARY_NAME).lib
	rm -f $(PDBFILE).pdb
