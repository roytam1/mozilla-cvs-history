DEPTH=..\..\..
#

LIBRARY_NAME = raptorwidget_s
REQUIRES=xpcom gfxwin raptor dom network netlib
DEFINES =-D_IMPL_NS_WIDGET -DMOZ_AIMM

CPPSRCS =              \
	nsFontRetrieverService.cpp      \
	nsFontSizeIterator.cpp      \
	nsNativeDragTarget.cpp      \
	nsNativeDragSource.cpp      \
	nsDragService.cpp      \
	Ienumfe.cpp      \
	nsDataObj.cpp      \
	nsDataObjCollection.cpp      \
	nsClipboard.cpp      \
	nsWindow.cpp         \
	nsButton.cpp         \
	nsTextWidget.cpp     \
	nsTextHelper.cpp     \
	nsFileWidget.cpp     \
	nsScrollbar.cpp      \
	nsAppShell.cpp       \
	nsLookAndFeel.cpp    \
	nsToolkit.cpp        \
	nsSound.cpp          \
	$(NULL)

MODULE=raptor 

OBJS =                             \
	.\$(OBJDIR)\nsFontRetrieverService.obj  \
	.\$(OBJDIR)\nsFontSizeIterator.obj  \
	.\$(OBJDIR)\nsNativeDragTarget.obj  \
	.\$(OBJDIR)\nsNativeDragSource.obj  \
	.\$(OBJDIR)\nsDragService.obj  \
	.\$(OBJDIR)\Ienumfe.obj  \
	.\$(OBJDIR)\nsDataObj.obj  \
	.\$(OBJDIR)\nsDataObjCollection.obj  \
	.\$(OBJDIR)\nsClipboard.obj  \
	.\$(OBJDIR)\nsWindow.obj         \
	.\$(OBJDIR)\nsButton.obj         \
	.\$(OBJDIR)\nsTextWidget.obj     \
	.\$(OBJDIR)\nsTextHelper.obj     \
	.\$(OBJDIR)\nsFileWidget.obj     \
	.\$(OBJDIR)\nsScrollbar.obj      \
	.\$(OBJDIR)\nsAppShell.obj       \
	.\$(OBJDIR)\nsLookAndFeel.obj    \
	.\$(OBJDIR)\nsToolkit.obj        \
	.\$(OBJDIR)\nsSound.obj          \
	$(NULL)

LINCS=                                  \
	-I$(PUBLIC)\raptor              \
	-I$(PUBLIC)\xpcom               \
	-I..\xpwidgets               \
	-I$(PUBLIC)\dom   \
	-I$(PUBLIC)\network   \
	-I$(PUBLIC)\netlib   \
	$(NULL)

LCFLAGS =                               \
	$(LCFLAGS)                      \
  $(DEFINES) \
	$(NULL)

WIN_LIBS=                        \
 ole32.lib                           

include <$(DEPTH)\config\rules.mak>

install:: $(LIBRARY)
	$(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib

clobber::
	rm -f $(DIST)\lib\$(LIBRARY_NAME).lib
	rm -f $(PDBFILE).pdb
