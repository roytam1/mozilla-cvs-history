DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

DIRS         = idl

MODULE       = zap_sip

EXTRA_PP_COMPONENTS = \
	SipSyntaxFactory.js \
	SipTransport.js \
	SipUAStack.js \
	SipTransactions.js \
	SipResolver.js \
	SipAuthentication.js \
	$(NULL)

include $(topsrcdir)/config/rules.mk

JS_CODELIB_RESOURCES = \
	SipUtils.js \
	SipUARequestCore.js \
	SipDialog.js \
	$(NULL)

libs::  $(JS_CODELIB_RESOURCES)
	$(INSTALL) $^ $(DIST)/bin/jscodelib/zap

install::  $(JS_CODELIB_RESOURCES)
	$(INSTALL) $^ $(DESTDIR)$(mozappdir)/bin/jscodelib/zap


