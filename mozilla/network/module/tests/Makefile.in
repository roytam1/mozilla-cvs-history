PROGRAM = nettest

CPPSRCS = \
	nettest.cpp \
	$(NULL)

LOCAL_INCLUDES := -I$(srcdir)/../src

REQUIRES = raptor netlib xpcom reg pref

LIBS = \
	-labouturl \
	-lfileurl \
	-lftpurl \
	-lgophurl \
	-lremoturl \
	-lhttpurl \
	-lsockstuburl \
	-lnetcache \
	-lmimetype \
	-lnetcnvts \
	-lnetwork \
	-lnetlib \
	-lreg	\
	-lxpcom	\
	-lpwcac \
	-l$(MOZ_LIB_DBM_PREFIX)dbm	\
	-lxp	\
	-lpref	\
	-l$(MOZ_LIB_JS_PREFIX)js	\
	-lsecfree \
	$(NSPR_LIBS) \
	$(TK_LIBS) \
	$(ZLIB_LIBS) \
	$(NULL)

include $(topsrcdir)/config/config.mk

include $(topsrcdir)/config/rules.mk

