  { 0x92, 0x40, 0xa9, 0x81, 0xe5, 0xab, 0xf1, 0x48 } } */

  CSSStyleDeclaration getOverrideStyle(in Element elt, 
                                       in DOMString pseudoElt);
};

// Introduced in DOM Level 2:
interface DocumentEvent {
/* IID: { 0x46b91d66, 0x28e2, 0x11d4, \
  { 0xab, 0x1e, 0x00, 0x10, 0x83, 0x01, 0x23, 0xb4 } } */

  Event createEvent(in DOMString eventType);
};

interface DocumentXBL {
/* IID: { 0xc7c0ae9b, 0xa0ba, 0x4f4e, \
  { 0x9f, 0x2c, 0xc1, 0x8d, 0xeb, 0x62, 0xee, 0x8b } } */

  NodeList getAnonymousNodes(in Element elt);
  void addBinding(in Element elt, in DOMString bindingURL);
  void removeBinding(in Element elt, in DOMString bindingURL);
};

interface NSDocument {
/* IID: { 0xa6cf90cd, 0x15b3, 0x11d2, \
  { 0x93, 0x2e, 0x00, 0x80, 0x5f, 0x8a, 0xdd, 0x32} } */

     readonly attribute long width;
     readonly attribute long height;
     readonly attribute DOMString characterSet;
// XXX This should be removed, the new createElementNS should be used in stead
     Element                   createElementWithNameSpace(in DOMString tagName,
                                                          in DOMString nameSpace)
                                                          raises(DOMException);
	 Range                     createRange();
	 void                      load(in DOMString url);
};

