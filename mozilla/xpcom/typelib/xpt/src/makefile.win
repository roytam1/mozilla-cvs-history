IGNORE_MANIFEST=1

DEPTH=..\..\..\..

MODULE	= 	xpcom

LIBRARY_NAME = xpcomxpt_s

LCFLAGS = -DEXPORT_XPT_API -DWIN32_LEAN_AND_MEAN

LINCS = \
	-I$(PUBLIC)\xpcom	\
	$(NULL)

LLIBS	=	\
		$(NULL)

CSRCS	=	\
        xpt_struct.c	\
		xpt_xdr.c	\
		$(NULL)

C_OBJS = \
	.\$(OBJDIR)\xpt_struct.obj		\
	.\$(OBJDIR)\xpt_xdr.obj		\
	$(NULL)

include <$(DEPTH)\config\rules.mak>

export:: $(LIBRARY)
	$(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib

clobber::
        rm -f $(DIST)\lib\$(LIBRARY_NAME).lib
