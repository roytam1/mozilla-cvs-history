DEPTH=..\..
MODULE = xpcom 
XPIDL_MODULE = xpcom_glue
LIBRARY_NAME=xpcomglue

################################################################################
## exports

EXPORTS =                  \
        nsCOMPtr.h         \
        $(NULL)			

XPIDLSRCS =               \
        $(NULL)

################################################################################

LCFLAGS = -D_IMPL_NS_COM -D_IMPL_NS_BASE -DWIN32_LEAN_AND_MEAN

CPP_OBJS =                            \
        .\$(OBJDIR)\nsCOMPtr.obj      \
        $(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(LIBRARY)
	$(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib

clobber::
        rm -f $(DIST)\lib\$(LIBRARY_NAME).lib
