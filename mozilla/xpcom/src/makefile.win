IGNORE_MANIFEST=1

MAKE_OBJ_TYPE	= DLL

DEPTH=..\..

LINCS = \
	-I$(PUBLIC)\libreg	\
	$(NULL)

LLIBS = \
	$(LIBNSPR)			\
	$(DIST)\lib\libplc21.lib	\
	$(DIST)\lib\libreg32.lib	\
	$(NULL)

LIBNAME = .\$(OBJDIR)\xpcom$(MOZ_BITS)
DLL = $(LIBNAME).dll

LCFLAGS = -D_IMPL_NS_COM -DUSE_NSREG -DWIN32_LEAN_AND_MEAN

CPPSRCS = \
	nsDebug.cpp		\
	nsHashtable.cpp		\
	nsID.cpp		\
	nsRepository.cpp	\
	nsSupportsArray.cpp	\

CPP_OBJS = \
	.\$(OBJDIR)\nsDebug.obj		\
	.\$(OBJDIR)\nsHashtable.obj	\
	.\$(OBJDIR)\nsID.obj		\
	.\$(OBJDIR)\nsSupportsArray.obj	\
	.\$(OBJDIR)\nsRepository.obj	\
	.\$(OBJDIR)\nsMalloc.obj	\

EXPORTS = \
	nsAgg.h			\
	nsCom.h			\
	nsDebug.h		\
	nsError.h		\
	nsHashtable.h		\
	nsID.h			\
	nsIFactory.h		\
	nsISupports.h		\
	nsIPtr.h		\
	nsISupportsArray.h	\
	nsRepository.h		\
	nsIEnumerator.h		\
	nsIBaseStream.h		\
	nsIInputStream.h	\
	nsIOutputStream.h	\
	nscore.h        	\
	nsIMalloc.h		\
	nsMalloc.h		\
	nsVector.h		\
	$(NULL)			

MODULE = xpcom 

include <$(DEPTH)\config\rules.mak>

libs:: $(DLL)
	$(MAKE_INSTALL) $(LIBNAME).$(DLL_SUFFIX) $(DIST)\bin
	$(MAKE_INSTALL) $(LIBNAME).$(LIB_SUFFIX) $(DIST)\lib


