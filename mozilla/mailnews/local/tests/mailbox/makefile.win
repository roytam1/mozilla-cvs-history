DEPTH=..\..\..\..
IGNORE_MANIFEST=1


MAKE_OBJ_TYPE   = EXE
PROGRAM = .\$(OBJDIR)\mailboxTest.exe

OBJS =										\
	.\$(OBJDIR)\mailboxTest.obj				\
	$(NULL)

LLIBS=							\
 $(DIST)\lib\xpcom.lib			\
 $(LIBNSPR)						\
 $(DIST)\lib\raptorgfxwin.lib	\
 $(DIST)\lib\raptorwidget.lib	\
 $(DIST)\lib\msgdb.lib			\
 $(DIST)\lib\msgbsutl.lib	\
 $(DIST)\lib\msglocal_s.lib		\
 $(NULL)

LLFLAGS= -SUBSYSTEM:CONSOLE 

include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAM)
	$(MAKE_INSTALL) $(PROGRAM) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\mailboxTest.exe

$(PROGRAM):: $(OBJS) $(MYLIBS)
