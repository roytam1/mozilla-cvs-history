include $(DEPTH)/config/autoconf.mk

PROGRAM		= msgcomposetest2

CPPSRCS		= test2.cpp

LIBS		= \
		$(MOZ_TIMER_LIBS) \
		$(NSPR_LIBS)	\
		$(XPCOM_LIBS) \
		-lmozjs \
