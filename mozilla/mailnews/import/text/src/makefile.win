DEPTH=..\..\..\..

MODULE=impText

MAKE_OBJ_TYPE=DLL
DLLNAME=$(MODULE)
DLL=.\$(OBJDIR)\$(DLLNAME).dll


CPP_OBJS=\
        .\$(OBJDIR)\nsTextFactory.obj			\
        .\$(OBJDIR)\nsTextStringBundle.obj		\
        .\$(OBJDIR)\nsTextImport.obj	        \
		.\$(OBJDIR)\nsTextAddress.obj			\
        $(NULL)

LLIBS=\
        $(DIST)\lib\xpcom.lib       \
        $(LIBNSPR)                              \
        $(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(DLL)
        $(MAKE_INSTALL) $(DLL) $(DIST)\bin\components

