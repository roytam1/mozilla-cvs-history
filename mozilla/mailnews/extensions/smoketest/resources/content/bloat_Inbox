    <key id="manBookmarkKb"        xulkey="true" key="&manBookmarksCmd.commandkey;" observes="Browser:ManageBookmark" />

	<!-- how to handle return, enter, tab, function keys, arrow keys, others? saari working on solution -->
</keyset>

