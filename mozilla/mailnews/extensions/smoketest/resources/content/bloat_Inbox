  <SCRIPT LANGUAGE=JAVASCRIPT TYPE="TEXT/JAVASCRIPT">
      <!-- Hide script from old browsers

      if (top.location != self.location) {
          top.location = self.location
      }
      // End hiding script from old browsers -->
  </SCRIPT>

