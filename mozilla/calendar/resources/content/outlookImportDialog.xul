<?xml-stylesheet href="chrome://global/skin/global.css" type="text/css"?>

<dialog xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
        xmlns:nc="http://home.netscape.com/NC-rdf#"
        id="calendar-import-outlook-fieldswindow"
        title="Map Outlook CSV fields"
        onload="loadOutlookImportDialog()"
        buttons="accept,cancel"
        ondialogaccept="return onOKCommand();"
        ondialogcancel="return onCancelCommand();">

  <script type="application/x-javascript" src="chrome://calendar/content/outlookImportDialog.js"/>

  <vbox>
    <label value="Please map the following concepts to their Outlook CSV counterparts:"/>
    <spacer height="15"/>
    <hbox>
      <spacer flex="1"/>
      <grid>
        <rows>
          <row align="center">
            <hbox pack="end">
              <label value="Subject"/>
            </hbox>
            <menulist id="title-list">
              <menupopup id="title-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Start date"/>
            </hbox>
            <menulist id="startdate-list">
              <menupopup id="startdate-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Start time"/>
            </hbox>
            <menulist id="starttime-list">
              <menupopup id="starttime-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="End date"/>
            </hbox>
            <menulist id="enddate-list">
              <menupopup id="enddate-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="End time"/>
            </hbox>
            <menulist id="endtime-list">
              <menupopup id="endtime-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Location"/>
            </hbox>
            <menulist id="location-list">
              <menupopup id="location-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Description"/>
            </hbox>
            <menulist id="description-list">
              <menupopup id="description-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="All-day event"/>
            </hbox>
            <menulist id="allday-list">
              <menupopup id="allday-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Private"/>
            </hbox>
            <menulist id="private-list">
              <menupopup id="private-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Alarm on/off"/>
            </hbox>
            <menulist id="alarm-list">
              <menupopup id="alarm-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Alarm date"/>
            </hbox>
            <menulist id="alarmdate-list">
              <menupopup id="alarmdate-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Alarm time"/>
            </hbox>
            <menulist id="alarmtime-list">
              <menupopup id="alarmtime-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <row align="center">
            <hbox pack="end">
              <label value="Categories"/>
            </hbox>
            <menulist id="categories-list">
              <menupopup id="categories-menupopup">
                <menuitem label="" value="0"/>
              </menupopup>
            </menulist>
          </row>
          <spacer height="15"/>
        </rows>
      </grid>
      <spacer flex="1"/>
    </hbox>
  </vbox>
  <label value="Please select the meaning of the following word:"/>
  <hbox>
    <spacer flex="1"/>
    <grid>
      <rows>
        <row align="center">
          <hbox pack="end">
            <label id="bool-label"/>
          </hbox>
          <menulist id="bool-list">
            <menupopup>
              <menuitem label="False" value="0"/>
              <menuitem label="True" value="1"/>
            </menupopup>
          </menulist>
        </row>
      </rows>
    </grid>
    <spacer flex="1"/>
  </hbox>
</dialog>