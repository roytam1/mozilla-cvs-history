    <!-- calendars uuid -->
    <em:id>{8e117890-a33f-424b-a2ea-deb272731365}</em:id>

    <!-- Our current version -->
    <em:version>0.8.2+</em:version>

    <em:targetApplication>
      <Description>
        <!-- Firefox -->
