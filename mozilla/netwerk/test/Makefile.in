DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

REQUIRES	= libreg xpcom

CPPSRCS		= \
		TestSocketIO.cpp \
		TestSocketInput.cpp \
		TestSocketTransport.cpp \
		TestFileInput.cpp \
		TestProtocols.cpp \
		urltest.cpp \
		TestFileInput2.cpp \
		TestRes.cpp \
		$(NULL)

SIMPLE_PROGRAMS	= $(CPPSRCS:.cpp=)

ifdef NO_LD_ARCHIVE_FLAGS
LOST_SYM_LIBS	= -lxpcomds_s -lxptinfo -lmozreg_s
endif

LIBS		= \
		-lmozjs \
		-lxpcom \
		$(LOST_SYM_LIBS) \
		$(NSPR_LIBS) \
		$(NULL)

include $(topsrcdir)/config/rules.mk

DEFINES		+= -DUSE_NSREG

