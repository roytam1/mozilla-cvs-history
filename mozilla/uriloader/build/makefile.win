DEPTH=..\..
MODULE=uriloader

!ifdef MOZ_STATIC_COMPONENT_LIBS
MAKE_OBJ_TYPE=LIB
LIBRARY_NAME = urildr
LIB = .\$(OBJDIR)\$(LIBRARY_NAME).lib
!else
LIBNAME = .\$(OBJDIR)\urildr
DLL = $(LIBNAME).dll
!endif

################################################################################
## library

LINCS=-I..\base                           \
      -I..\exthandler                     \
      -I..\exthandler\win                 \
      $(NULL)

CPP_OBJS=									                \
			.\$(OBJDIR)\nsURILoaderModule.obj   \
			$(NULL)

LLIBS=										                \
      $(DIST)\lib\exthandlers_s.lib       \
			$(DIST)\lib\xpcom.lib			          \
      $(DIST)\lib\uriloaderbase_s.lib     \
			$(LIBNSPR)						              \
      $(DIST)\lib\js3250.lib	\
			$(NULL)

WIN_LIBS= shell32.lib

include <$(DEPTH)/config/rules.mak>

!ifdef MOZ_STATIC_COMPONENT_LIBS
install:: $(LIB)
	$(MAKE_INSTALL)  $(LIB) $(DIST)\bin\components
	$(MAKE_INSTALL)  $(LIB) $(DIST)\lib
!else
install:: $(DLL)
	$(MAKE_INSTALL)  $(LIBNAME).$(DLL_SUFFIX) $(DIST)\bin\components
	$(MAKE_INSTALL)  $(LIBNAME).$(LIB_SUFFIX) $(DIST)\lib
!endif

