DEPTH=..\..\..

MAKE_OBJ_TYPE	= EXE
PROG0 = .\$(OBJDIR)\StringBundleTest.exe
PROGRAMS = $(PROG0)

LINCS=\
    -I$(PUBLIC)\intl \
    -I$(PUBLIC)\netlib \
    -I$(PUBLIC)\raptor \
    -I$(PUBLIC)\xpcom \
    -I$(PUBLIC)\locale \
    $(NULL)

LLIBS=				\
 $(DIST)\lib\xpcom.lib	\
 $(LIBNSPR)			\
!if "$(MOZ_BITS)"=="32" && defined(MOZ_DEBUG) && defined(GLOWCODE)
 $(GLOWDIR)\glowcode.lib	\
!endif
 $(RESFILE)

include <$(DEPTH)\config\rules.mak>

DISTBROWSWER=$(DIST)\bin\chrome\global\content

install:: $(PROGRAMS)
	$(MAKE_INSTALL) $(PROG0) $(DIST)\bin
	$(MAKE_INSTALL) strres.properties $(DIST)\bin\res
	$(MAKE_INSTALL) strres-test.html $(DIST)\bin\res
	$(MAKE_INSTALL) strres-test.xul $(DIST)\bin\res
	$(MAKE_INSTALL) strres-test.dtd $(DIST)\bin\res
	$(MAKE_INSTALL) strres-test.js $(DIST)\bin\res

clobber::
	rm -f $(DIST)\bin\StringBundleTest.exe
	rm -f $(DIST)\bin\res\strres.properties
	rm -f $(DIST)\bin\res\strres-test.html
	rm -f $(DIST)\bin\res\strres-test.xul
	rm -f $(DIST)\bin\res\strres-test.dtd
	rm -f $(DIST)\bin\res\strres-test.js

$(PROG0): $(OBJDIR) StringBundleTest.cpp
