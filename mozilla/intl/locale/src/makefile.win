DEPTH=..\..\..
IGNORE_MANIFEST=1

#DIRS = windows

LIBRARY_NAME=locale_s
MODULE=locale 
REQUIRES=xpcom

INCS=-I$(PUBLIC)\xpcom -I$(PUBLIC)\locale -I$(PUBLIC)\unicharutil -I$(PUBLIC)\raptor

DEFINES=-DWIN32_LEAN_AND_MEAN

CPPSRCS=nsCollation.cpp

EXPORTS=nsCollation.h

CPP_OBJS=.\$(OBJDIR)\nsCollation.obj

LINCS= \
	$(NULL)

LCFLAGS = \
        $(LCFLAGS) \
        $(DEFINES) \
        $(NULL)

include <$(DEPTH)\config\rules.mak>

libs:: $(LIBRARY)
	$(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib

clobber::
	rm -f $(DIST)\bin\$(DLLNAME).lib
