DEPTH		= ../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= ucvlatin
LIBRARY_NAME	= ucvlatin
IS_COMPONENT	= 1

CSRCS		=  \
		  $(NULL)

CPPSRCS		= \
                  nsLatin1ToUnicode.cpp \
                  nsISO88597ToUnicode.cpp \
                  nsCP1253ToUnicode.cpp \
                  nsUTF8ToUnicode.cpp \
                  nsUnicodeToLatin1.cpp \
                  ns1ByteToUnicodeBase.cpp \
		  nsUCvLatinSupport.cpp \
                  nsUCvLatinDll.cpp \
		  $(NULL)

EXPORTS         = \
                  nsUCvLatinCID.h \
		  $(NULL)
                
EXPORTS		:= $(addprefix $(srcdir)/, $(EXPORTS))

CFLAGS		+= -DUSE_NSREG -D__STDC__

REQUIRES	= $(MODULE) libxpcom libraptorbase

include $(topsrcdir)/config/rules.mk
