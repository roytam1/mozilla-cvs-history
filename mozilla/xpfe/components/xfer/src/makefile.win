LIBRARY_NAME	= mozxfer
MODULE_NAME	= nsStreamTransferModule

LLIBS	= \
	$(LIBNSPR) \
	$(DIST)\lib\xpcom.lib \
	$(DIST)\lib\js3250.lib \
	$(NULL)

#//------------------------------------------------------------------------
