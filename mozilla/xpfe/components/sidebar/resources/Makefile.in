DEPTH		= ../../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

CHROME_DIR	= sidebar
CHROME_CONTENT_DIR = content/default
CHROME_SKIN_DIR	= skin/default
CHROME_SOURCE_DIR = $(srcdir)

CHROME_CONTENT	= \
		customize-panel.js \
		customize-panel.xul \
		customize.js \
		customize.xul \
		dummy-flash.rdf \
		flash-registry.rdf \
		flash.js \
		flash.xul \
		preview.css \
		preview.js \
		preview.xul \
		sidebarOverlay.js \
		sidebarOverlay.xul \
		local-panels.rdf \
		remote-panels.rdf \
		default-panels.rdf \
		$(NULL)

CHROME_SKIN	= \
		customize-panel.css \
		customize.css \
		flash.css \
		sidebarOverlay.css \
		flames.gif \
		list-down-dis.gif \
		list-down.gif \
		list-up-dis.gif \
		list-up.gif \
		panel-expand.gif \
		panel-collapse.gif \
		$(NULL)

CHROME_L10N	= \
		./locale/en-US/customize-panel.dtd \
		./locale/en-US/customize.dtd \
		./locale/en-US/flash.dtd \
		./locale/en-US/preview.dtd \
		./locale/en-US/sidebar-registry-rdf.dtd \
		./locale/en-US/sidebar-rdf.dtd \
		./locale/en-US/sidebarOverlay.dtd \
		$(NULL)

include $(topsrcdir)/config/rules.mk

