LIBS = \
	-L$(DIST)/bin	\
	-lnsappshell	\
	-lxpcom		\
	-lraptorbase	\
	$(TOOLKIT_WIDGET_LIB)\
	-lraptorgfx	\
	$(TOOLKIT_GFX_LIB)\
	-lgfxps         \
	$(TOOLKIT_BASE_LIB)\
	-lreg		\
	-labouturl	\
	-lhttpurl	\
	-lsockstuburl 	\
	-lfileurl	\
	-lgophurl	\
	-lftpurl	\
	-lremoturl	\
	-lxp		\
	-lnetutil	\
	-lnetcache	\
	-lnetcnvts	\
	-lmimetype	\
	-lnetwork	\
	-lnetlib	\
	-lraptorwebwidget	\
	-lraptorhtml	\
	-lraptorhtmlpars	\
	-ljsdom		\
	-ljsurl		\
	-lraptorbase	\
