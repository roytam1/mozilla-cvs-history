DEPTH=..\..
IGNORE_MANIFEST=1

DEFINES=-DWIN32_LEAN_AND_MEAN
MODULE=raptor 

CPPSRCS=                                \
 registerFactories.cpp                  \
 $(NULL)

CPP_OBJS=                               \
 .\$(OBJDIR)\registerFactories.obj      \
 $(NULL)

LINCS=-I$(PUBLIC)\raptor                \
	  -I$(PUBLIC)\xpcom                 \
      -I$(PUBLIC)\base                  \
	  $(NULL)

MAKE_OBJ_TYPE = EXE
PROGRAM = .\$(OBJDIR)\registerFactories.exe

LCFLAGS = \
        $(LCFLAGS) \
        $(DEFINES) \
        $(NULL)

# These are the libraries we need to link with to create the exe
LLIBS=                          \
 $(DIST)\lib\xpcom32.lib        \
 $(DIST)\lib\libplc3.lib       \
 $(LIBNSPR)                     \
 $(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAM)
	$(MAKE_INSTALL) $(PROGRAM) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\registerFactories.exe
