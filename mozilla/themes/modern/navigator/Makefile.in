	forward.gif \
	home.gif \
	location-hover.gif \
	location.gif \
	messagel.gif \
	mynetscape.gif \
	n-box-persbar.gif \
	navbar-inner-groove-left.gif \
	navbar-inner-groove-middle.gif \
	navbar-inner-groove-right.gif \
	navigator.css \
	print.gif \
	questionl.gif \
	reload-clicked.gif \
	reload-disabled.gif \
	reload-hover.gif \
	reload.gif \
	search.gif \
	stop-clicked.gif \
	stop-disabled.gif \
	stop-hover.gif \
	stop.gif \
	ubhist-keyword-popup-active.gif \
	ubhist-keyword-popup.gif \
	urlbar-left.gif \
