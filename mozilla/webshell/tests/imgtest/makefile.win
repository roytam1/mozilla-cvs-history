DEPTH=..\..\..

MAKE_OBJ_TYPE	= EXE
PROGRAM = .\$(OBJDIR)\ImageTest.exe
RESFILE = image.res
MAPFILE = image.map

LINCS=-I$(XPDIST)\public\raptor -I$(XPDIST)\public\xpcom -I..\..\src

LLFLAGS = $(LLFLAGS) /SUBSYSTEM:CONSOLE

OBJS =                                  \
	.\$(OBJDIR)\ImageTest.obj         \
	$(NULL)

LLIBS=				\
 $(DIST)\lib\raptorgfxwin.lib 	\
 $(DIST)\lib\raptorweb.lib		\
 $(DIST)\lib\xpcom.lib		\
 $(LIBNSPR)			\
 $(DIST)\lib\img32$(VERSION_NUMBER).lib		\
 $(DIST)\lib\util.lib

WIN_LIBS= comdlg32.lib                 

include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAM)
	$(MAKE_INSTALL) $(PROGRAM) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\ImageTest.exe

