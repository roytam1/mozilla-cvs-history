DEPTH=..\..\..

include <$(DEPTH)/config/config.mak>

DIRS=public windows

MAKE_OBJ_TYPE   = EXE
PROGRAM = .\$(OBJDIR)\viewer.exe
RESFILE = viewer.res
!ifdef MOZ_DEBUG
RCFLAGS=/d_DEBUG
!endif

!ifdef MOZ_DEBUG
RCFLAGS=/dPURIFY
LCFLAGS=-DPURIFY
!endif

LCFLAGS = $(LCFLAGS) -DUSE_LOCAL_WIDGETS

OBJS =                                      \
	.\$(OBJDIR)\nsBaseDialog.obj     \
