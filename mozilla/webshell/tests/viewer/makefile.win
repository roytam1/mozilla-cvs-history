	.\$(OBJDIR)\nsWinMain.obj           \
	.\$(OBJDIR)\JSConsole.obj           \
	.\$(OBJDIR)\nsWidgetSupport.obj           \
	$(NULL)

LINCS=  \
  -I$(PUBLIC)\raptor \
  -I$(PUBLIC)\xpcom \
  -I$(PUBLIC)\dom \
  -I$(PUBLIC)\js \
!ifdef NECKO
