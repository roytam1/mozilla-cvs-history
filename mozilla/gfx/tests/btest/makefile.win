DEPTH=..\..\..

MAKE_OBJ_TYPE	= EXE
PROG0 = .\$(OBJDIR)\BitTest.exe
RESFILE = image.res
PROGRAMS = $(PROG0)

LINCS=-I$(XPDIST)\public\raptor -I$(XPDIST)\public\xpcom -I..\src

LLIBS=				\
 $(DIST)\lib\gkgfxwin.lib		\
 $(DIST)\lib\xpcom.lib		\
 $(LIBNSPR)			\
 $(DIST)\lib\img32$(VERSION_NUMBER).lib		\
 $(DIST)\lib\util.lib 		 \
 $(RESFILE)

WIN_LIBS=comdlg32.lib

include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAMS)
	$(MAKE_INSTALL) $(PROG0) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\BitTest.exe

$(PROG0): $(OBJDIR) BitTest.cpp $(RESFILE)
