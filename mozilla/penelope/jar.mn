  content/penelope/toolbarOverlay.xul                                (base/content/toolbarOverlay.xul)
  content/penelope/customizeToolbar.xul                              (base/content/customizeToolbar.xul)
  content/penelope/customizeToolbar.js                               (base/content/customizeToolbar.js)
  content/penelope/toolbarbuttons.js                                 (base/content/toolbarbuttons.js)
  content/penelope/toolbar.js                                        (base/content/toolbar.js)
  content/penelope/quotes.js                                         (base/content/quotes.js)
  content/penelope/help.js                                           (base/content/help.js)
