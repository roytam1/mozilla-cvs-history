DEPTH		= ../../../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

DIRS		= \
		include \
		$(NULL)

MODULE		= moz_shunt_ppc2002
LIBRARY_NAME	= moz_shunt_ppc2002
FORCE_SHARED_LIB	= 1

CPPSRCS		= \
		a2w.cpp \
		assert.cpp \
		direct.cpp \
		errno.cpp \
		io.cpp \
		mbstring.cpp \
		moz_shunt_ppc2002.cpp \
		process.cpp \
		signal.cpp \
		stat.cpp \
		stdio.cpp \
		stdlib.cpp \
		string.cpp \
		time.cpp \
		w2a.cpp \
		win32.cpp \
		win32A.cpp \
		win32W.cpp \
		$(NULL)

DEFINES		+= \
		-DMOZ_SHUNT_PPC2002_EXPORTS \
        $(NULL)

#
# Custom OS_LIBS for this one particular DLL.
#
OS_LIBS		= \
		corelibc.lib \
        coredll.lib \
        winsock.lib \
		$(NULL)

include $(topsrcdir)/config/rules.mk

