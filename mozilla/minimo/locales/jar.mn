#filter substitution


@AB_CD@.jar:
%   locale  minimo   @AB_CD@       %locale/@AB_CD@/minimo/
%   locale  branding @AB_CD@       %locale/@AB_CD@/branding/
	locale/@AB_CD@/minimo/minimo.dtd	      (%chrome/minimo.dtd)
    locale/@AB_CD@/minimo/minimo.properties (%chrome/minimo.properties)
    locale/@AB_CD@/minimo/preferences.dtd   (%chrome/preferences.dtd)
    locale/@AB_CD@/minimo/bookmarks.dtd     (%chrome/bookmarks.dtd)
    locale/@AB_CD@/minimo/history.dtd       (%chrome/history.dtd)
    locale/@AB_CD@/minimo/rssload.dtd       (%chrome/rssload.dtd)
    branding/locale/brand.dtd        (%chrome/brand.dtd)
    branding/locale/brand.properties (%chrome/brand.properties)
