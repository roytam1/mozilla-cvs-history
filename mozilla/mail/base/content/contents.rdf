        chrome:author="mozilla.org"
        chrome:name="mail"
        chrome:localeVersion="1.2a"
        chrome:skinVersion="1.2">
  </RDF:Description>

  <!-- overlay information -->
  <RDF:Seq about="urn:mozilla:overlays">
