DEPTH=..\..\..\..

MAKE_OBJ_TYPE   = EXE
USE_NON_MT_LIBS = 1

MODULE          = ds32
PROGRAM         = .\$(OBJDIR)\$(MODULE).exe

OBJS =                       \
    .\$(OBJDIR)\ds32.obj     \
    $(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAM)
    if not exist $(MOZ_TOOLS)\bin\nul mkdir $(MOZ_TOOLS)\bin
    copy $(PROGRAM) $(MOZ_TOOLS)\bin

clobber_all::
    $(RM) $(MOZ_TOOLS)\bin\$(MODULE).exe

