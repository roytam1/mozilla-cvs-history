DEPTH=..\..\..\..

MAKE_OBJ_TYPE=DLL

USE_NON_MT_LIBS=1
#USE_STATIC_LIBS=1

MODULE      = setuprsc
RESFILE     = $(MODULE).res
DLL         = $(OBJDIR)\$(MODULE).dll
EXPORTS     = setuprsc.h

OBJS=                             \
    .\$(OBJDIR)\setuprsc.obj      \
    $(NULL)

include <$(DEPTH)/config/rules.mak>

install:: $(DLL)
    $(MAKE_INSTALL) .\$(OBJDIR)\$(MODULE).dll $(DIST)\install

clobber::
  $(RM) $(DIST)\include\setuprsc.h

clobber_all:: clobber
  $(RM) $(DIST)\install\$(MODULE).dll

