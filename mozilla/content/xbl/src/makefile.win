      string \
      js \
      dom \
      widget \
      view \
      caps \
      layout_xul \
      htmlparser \
      necko \
      xpconnect \
      pref \
      docshell \
      webshell \
      chrome \
      lwbrk \
      gfx2 \
      imglib2 \
      layout \
      gfx \
      rdf \
      content_xul \
      unicharutil \
