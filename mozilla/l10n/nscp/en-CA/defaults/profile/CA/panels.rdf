<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:NC="http://home.netscape.com/NC-rdf#">

  <RDF:Description about="urn:sidebar:current-panel-list">
    <NC:panel-list>
      <RDF:Seq>
        <RDF:li resource="urn:sidebar:panel:search" />
        <RDF:li resource="urn:sidebar:panel:bookmarks" />
        <RDF:li resource="urn:sidebar:panel:im-panel" />      
        <RDF:li resource="urn:sidebar:nc-panel:channels" />
        <RDF:li resource="urn:sidebar:nc-panel:mapsndirections" />
        <RDF:li resource="urn:sidebar:panel:history" />
        <RDF:li resource="urn:sidebar:panel:addressbook" />
      </RDF:Seq>
    </NC:panel-list>
    <NC:version>0.1</NC:version>
  </RDF:Description>

  <RDF:Description about="urn:sidebar:panel:search">
    <NC:title>&sidebar.panel.search;</NC:title>
    <NC:content>chrome://communicator/content/search/search-panel.xul</NC:content>
    <NC:exclude>mail:3pane Aim:AimApp Aim:AimChat mail:addressbook composer:html</NC:exclude>
  </RDF:Description>
  <RDF:Description about="urn:sidebar:panel:bookmarks">
    <NC:title>Bookmarks</NC:title>
    <NC:content>chrome://communicator/content/bookmarks/bm-panel.xul</NC:content>
    <NC:exclude>mail:3pane Aim:AimApp Aim:AimChat mail:addressbook composer:html</NC:exclude>
  </RDF:Description>
  <RDF:Description about="urn:sidebar:panel:im-panel">
    <NC:title>&sidebar.im-panel.label;</NC:title>
    <NC:content>chrome://aim/content/SidebarPanel.xul</NC:content>
  </RDF:Description>
  <RDF:Description about="urn:sidebar:nc-panel:channels">
    <NC:title>&sidebar.nc-panel.channels;</NC:title>
    <NC:content>http://www.netscape.ca/browsers/7/sidebar/hotspot/index.jsp</NC:content>
    <NC:exclude>mail:3pane Aim:AimApp Aim:AimChat mail:addressbook composer:html</NC:exclude>
  </RDF:Description>
  <RDF:Description about="urn:sidebar:nc-panel:mapsndirections">
    <NC:title>&sidebar.nc-panel.mapsndirections;</NC:title>
    <NC:content>http://info.netscape.com/fwd/sidusmap/http://www.netscape.ca/maps/sidebar/</NC:content>
    <NC:exclude>mail:3pane Aim:AimApp Aim:AimChat mail:addressbook composer:html</NC:exclude>
  </RDF:Description>
   <RDF:Description about="urn:sidebar:panel:history">
    <NC:title>History</NC:title>
    <NC:content>chrome://communicator/content/history/history-panel.xul</NC:content>
    <NC:exclude>navigator:browser mail:3pane Aim:AimApp Aim:AimChat mail:addressbook composer:html</NC:exclude>
  </RDF:Description>
  <RDF:Description about="urn:sidebar:panel:addressbook">
    <NC:title>&sidebar.panel.addressbook;</NC:title>
    <NC:content>chrome://messenger/content/addressbook/addressbook-panel.xul</NC:content>
    <NC:exclude>navigator:browser Aim:AimApp Aim:AimChat mail:addressbook composer:html</NC:exclude>
  </RDF:Description>
</RDF:RDF>
