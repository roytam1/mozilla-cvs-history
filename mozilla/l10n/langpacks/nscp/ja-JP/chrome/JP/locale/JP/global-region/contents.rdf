<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the locale being supplied by this package -->
  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:JP"/>
  </RDF:Seq>

  <!-- locale information -->
  <RDF:Description about="urn:mozilla:locale:JP"
        chrome:displayName="Japan Region"
        chrome:author="mozilla.org"
        chrome:name="JP"
        chrome:localeType="region"
        chrome:localeVersion="1.0.0rc2"
        chrome:previewURL="http://www.mozilla.org/locales/JP.gif">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:JP:packages">
        <RDF:li resource="urn:mozilla:locale:JP:global-region"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

  <!-- Version Information.  State that we work only with major version of this
       package. -->
  <RDF:Description about="urn:mozilla:locale:JP:global-region"
	chrome:localeVersion="1.0.0rc2"/>
</RDF:RDF>
