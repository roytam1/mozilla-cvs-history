DEPTH			= ../../../..

# XfeWidgets headers are exported to dist/public/XfePref/Xfe.  We use
# the 'Xfe' directory to avoid the hasstle of having to remember yet another
# include prefix.
MODULE			= XfePref/Xfe

LIBRARY_NAME	= XfePref

CSRCS			= \
				PrefGroup.c \
				PrefPanel.c \
				PrefPanelInfo.c \
				PrefView.c \
				$(NULL)

REQUIRES		= \
				XfeWidgets \
				XfePref \
				Microline

EXPORTS			= \
				PrefGroup.h \
				PrefGroupP.h \
				PrefItem.h \
				PrefItemP.h \
				PrefPanel.h \
				PrefPanelInfo.h \
				PrefPanelInfoP.h \
				PrefPanelP.h \
				PrefView.h \
				PrefViewP.h \
				$(NULL)

include $(DEPTH)/config/rules.mk
