getheaders:
  -rm -rf include
  mkdir include

  cd windows
  -rm -rf include
  mkdir include
  cd include
  mkdir obsolete
  cd ..\..

  -rm -rf _tmp
  mkdir _tmp
  cd _tmp

  cvs co mozilla/xpcom/base/nsCOMPtr.h
  cvs co mozilla/xpcom/base/nsDebug.h
  cvs co mozilla/xpcom/base/nsError.h
  cvs co mozilla/xpcom/base/nsIAllocator.h
