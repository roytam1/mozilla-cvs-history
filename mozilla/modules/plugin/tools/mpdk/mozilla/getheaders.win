  ..\..\..\..\windows\bin\xpidl -m header nsISupports.idl
  mv *.h ../../../../include
  cd ..\..\..

  cvs co mozilla/xpcom/ds/nsCRT.h
  cvs co mozilla/xpcom/ds/nsCppSharedAllocator.h
  cvs co mozilla/xpcom/ds/nsStr.h
  cvs co mozilla/xpcom/ds/nsString.h
