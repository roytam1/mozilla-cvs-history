DEPTH=..\..\..\..
include <$(DEPTH)/config/config.mak>


!ifdef MOZ_STATIC_COMPONENT_LIBS
MAKE_OBJ_TYPE	= LIB
LIBRARY_NAME = npnul32
LIB=.\$(OBJDIR)\$(LIBRARY_NAME).lib
!else
MAKE_OBJ_TYPE	= DLL
DLLNAME = npnul32
DLL=.\$(OBJDIR)\$(DLLNAME).dll
!endif

RESFILE = npnul32.res
DEFFILE = npnul32.def

MODULE=npnul32 

DEFINES =-D_IMPL_NS_PLUGIN -DWIN32_LEAN_AND_MEAN

OBJS =                              \
	.\$(OBJDIR)\maindll.obj \
	.\$(OBJDIR)\plugin.obj \
	.\$(OBJDIR)\dbg.obj \
	.\$(OBJDIR)\dialogs.obj \
	.\$(OBJDIR)\npshell.obj \
	.\$(OBJDIR)\npwin.obj \
	.\$(OBJDIR)\utils.obj \
	$(NULL)


LCFLAGS = \
        $(LCFLAGS) \
        -D_IMPL_NS_PLUGIN \
        -GX \
        $(NULL)

WIN_LIBS = \
  version.lib

include <$(DEPTH)\config\rules.mak>

it:
	echo $(LLIBS)

!ifdef MOZ_STATIC_COMPONENT_LIBS
install:: $(LIB)
	$(MAKE_INSTALL) .\$(OBJDIR)\$(LIBRARY_NAME).lib $(DIST)\bin\plugins

clobber::
	rm -f $(DIST)\bin\$(LIBRARY_NAME).lib
!else
install:: $(DLL)
	$(MAKE_INSTALL) .\$(OBJDIR)\$(DLLNAME).dll $(DIST)\bin\plugins
 
clobber::
	rm -f $(DIST)\bin\$(DLLNAME).dll
!endif
