CSRCS		= \
		mng_callback_xs.c \
		mng_chunk_io.c \
		mng_chunk_prc.c \
		mng_chunk_xs.c \
		mng_cms.c \
		mng_display.c \
		mng_dither.c \
		mng_error.c \
		mng_filter.c \
		mng_hlapi.c \
		mng_jpeg.c \
		mng_object_prc.c \
		mng_pixels.c \
		mng_prop_xs.c \
		mng_read.c \
		mng_trace.c \
		mng_write.c \
		mng_zlib.c \
		$(NULL)

EXPORTS		= libmng.h mng_conf.h mng_types.h

EXPORTS		:= $(addprefix $(srcdir)/, $(EXPORTS))

LOCAL_INCLUDES	= -I$(srcdir)

override NO_SHARED_LIB=1
override NO_STATIC_LIB=

include $(topsrcdir)/config/rules.mk

