
cssinspector.jar:
% content   cssinspector    			%content/cssinspector/
% skin		cssinspector    classic/1.0 %skin/classic/cssinspector/
% overlay   chrome://composer/content/composer.xul		chrome://cssinspector/content/composerOverlay.xul
	content/cssinspector/composerOverlay.xul			(content/composerOverlay.xul)
	content/cssinspector/cssinspector.xul				(content/cssinspector.xul)
	content/cssinspector/cssinspector.js				(content/cssinspector.js)
	skin/classic/cssinspector/cssinspector.css			(skin/cssinspector.css)

cssinspector-locale.jar:
% locale    cssinspector    en-US		%locale/en-US/cssinspector/
	locale/en-US/cssinspector/composerOverlay.dtd		(locale/en-US/composerOverlay.dtd)
