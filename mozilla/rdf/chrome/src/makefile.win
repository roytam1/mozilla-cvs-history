DEPTH=..\..\..
MODULE=chrome
LIBRARY_NAME=chrome_s
REQUIRES	= xpcom \
		  string \
		  necko \
		  rdf \
		  appshell \
		  layout \
		  content \
		  intl \
		  dom \
		  widget \
		  content_xul \
		  gfx \
		  docshell \
		  pref \
		  imglib2 \
		  caps \
		  xpconnect \
		  js \
		  $(NULL)
CPP_OBJS=\
        .\$(OBJDIR)\nsChromeRegistry.obj    \
	    .\$(OBJDIR)\nsChromeProtocolHandler.obj    \
      .\$(OBJDIR)\nsChromeUIDataSource.obj \
	$(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(LIBRARY)
        $(MAKE_INSTALL) $(LIBRARY) $(DIST)\lib

clobber::
        rm -f $(DIST)\lib\$(LIBRARY_NAME).lib




